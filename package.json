{
	"name": "point-of-sale-system",
	"version": "1.0.0",
	"description": "a point-of-sale-ssytem with quality of life features added for small restaurant use",
	"main": "server/server.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "cd server && npm start",
		"start:prod": "cd server && npm start",
		"start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
		"install": "cd server && npm i && cd ../client && npm i",
		"seed": "cd server && npm run seed",
		"heroku-postbuild": "cd client && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BrigittePowers/point-of-sale-system.git"
	},
	"author": "Brigitte Powers",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/BrigittePowers/point-of-sale-system/issues"
	},
	"homepage": "https://github.com/BrigittePowers/point-of-sale-system#readme",
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"concurrently": "^7.0.0"
	}
}
